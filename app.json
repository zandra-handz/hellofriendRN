{
  "expo": {
    "name": "sampleproject",
    "slug": "sampleproject",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.badrainbowz.sampleproject",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBAW09hdzlszciQ4fTiZjfxcVMlEkF5Iqk"
        }
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": { 
      "icon": "./assets/shapes/lizard.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3564e25f-85ac-4c60-9f14-93d9153871b3"
      }
    }
  }
}

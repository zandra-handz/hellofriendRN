{
  "expo": {
    "name": "sampleproject",
    "slug": "sampleproject",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.badrainbowz.sampleproject",  
      "buildNumber": "1.0.0",                        
      "supportsTablet": true,                                
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show your current position on the map." // Example permission
      },
      "config": {
        "usesNonExemptEncryption": false                 
      }
    },
    "android": {  
      "package": "com.badrainbowz.sampleproject",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyArVY_-EOJjalOpYmc-XSIk-vmPvr9qnKs"
        }
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }, 
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3564e25f-85ac-4c60-9f14-93d9153871b3"
      }
    }
  }
}
